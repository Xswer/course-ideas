{{#partial "content"}}
    <h1>Those people have voted for {{ idea.title }}</h1>
        <h2>{{ idea.voteCount }} Voters: </h2>

    <ul>
        {{#each idea.voters}}
            <li>
                {{ this }}
            </li>
        {{/each}}
    </ul>

    <form action="/ideas/{{ idea.slug }}/vote" method="post">
        <button>VOTE</button>
    </form>
{{/partial}}

{{#partial "title"}}
    Course Idea - {{ idea.title }}
{{/partial}}




{{> base.hbs}}